{
  "name": "puppeteer-dsl",
  "version": "0.0.16",
  "description": "An intuitive DSL for Puppeteer, simplifying web automation and testing. Currently in alpha, subject to changes.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "types": "./dist/mjs/puppeteerDSL.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/puppeteerDSL.js",
      "require": "./dist/cjs/puppeteerDSL.js"
    }
  },
  "scripts": {
    "build": "tsc --declaration --outDir dist/mjs && tsc --module commonjs --outDir dist/cjs",
    "test": "jest",
    "start": "node dist/cjs/index.js"
  },
  "homepage": "https://github.com/311ecode/puppeteer-dsl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/311ecode/puppeteer-dsl.git"
  },
  "keywords": [
    "puppeteer",
    "dsl",
    "automation",
    "testing",
    "web-scraping",
    "browser",
    "headless-chrome"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/311ecode/puppeteer-dsl/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@types/debug": "^4.1.12",
    "@types/puppeteer": "^7.0.4",
    "debug": "^4.3.5",
    "dsl-framework": "^1.8.33",
    "puppeteer": "^22.13.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
